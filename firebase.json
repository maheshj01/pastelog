{
  "hosting": {
    "target": "pastelog",
    "public": ".next/static",
    "site": "pastelog",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "pastelog",
          "region": "us-central1"
        }
      },
      {
        "source": "/logs/**",
        "destination": "/logs/[id]/index.html"
      }
    ]
  }
}