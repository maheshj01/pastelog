{
   "files.autoSaveDelay": 3000,
   "editor.formatOnSave": true,
   "files.exclude": {
      //  "build":true,
      "test": true,
      //    ".vscode":false,
      //    ".*":true,
      "web": false,
      "build": false,
      "windows": true,
      "macos": true,
   },
